Adding alarm feature

import tkinter as tk
from tkinter import messagebox
from datetime import datetime
import threading
import time
import winsound  # Works on Windows; for Linux/macOS we can change it later

class AlarmClock(tk.Tk):
    def __init__(self):
        super().__init__()
        self.title("Alarm Clock")
        self.configure(bg="#111")

        # Time display
        self.time_label = tk.Label(self, font=("Segoe UI", 48, "bold"),
                                   fg="#e6e6e6", bg="#111")
        self.time_label.pack(pady=20)

        # Alarm time entry
        frame = tk.Frame(self, bg="#111")
        frame.pack(pady=10)

        tk.Label(frame, text="Set Alarm (HH:MM:SS):",
                 fg="#e6e6e6", bg="#111", font=("Segoe UI", 14)).pack(side=tk.LEFT, padx=5)

        self.alarm_entry = tk.Entry(frame, font=("Segoe UI", 14), width=10, justify="center")
        self.alarm_entry.pack(side=tk.LEFT)

        self.set_btn = tk.Button(self, text="Set Alarm",
                                 command=self.set_alarm,
                                 bg="#222", fg="#e6e6e6",
                                 font=("Segoe UI", 12),
                                 relief="flat", padx=10, pady=5)
        self.set_btn.pack(pady=10)

        self.alarm_time = None
        self.running = True

        # Start clock update
        self.update_clock()

        # Start alarm check thread
        threading.Thread(target=self.check_alarm, daemon=True).start()

